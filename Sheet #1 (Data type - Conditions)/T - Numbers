#include<bits/stdc++.h>
using namespace std;
 
int main(){
    int arr[3];
    int sot[3];
    
    for(int i=0;i<3;i++){
        cin>>arr[i];
        sot[i]=arr[i];
    }
    sort(arr,arr+3);
    for(int i=0;i<3;i++){
        cout<<arr[i]<<"\n";
    }
    
    cout<<"\n";
    for(int i=0;i<3;i++){
        cout<<sot[i]<<"\n";
    }
    return 0;
}
